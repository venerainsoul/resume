<div>
  {{#with main}}
    <main>
      <section>
        {{> title data='My educationss' big=true}}
        <ul>
          {{#each education}}
            <li>
              {{name}}
              {{#unless isEnd}} (ще в процесі) {{/unless}}
            </li>
          {{/each}}
        </ul>
      </section>
      <br />
      <section>
        {{> title data='My certificates' big=false}}
        <ul>
          {{#each certificates}}
            <li>{{name}} ({{id}})</li>
          {{/each}}
        </ul>
      </section>

    </main>
  {{/with}}
</div>